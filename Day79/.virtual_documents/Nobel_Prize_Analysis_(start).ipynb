# Lets convert prize share column with values such as 1/2 ,1 to 50,100
def convert(x):
    val = x.split('/')
    per = round(int(val[0])/int(val[1]),4)*100
    return per





df_data['share_pct']=df_data['prize_share'].map(lambda x:convert(x))


